<SCRIPT language=javascript><!--

function reinitIframe1(){ 
var iframe = document.getElementById("userpageleft"); 
try{ 
var bHeight = iframe.contentWindow.document.body.scrollHeight; 
var dHeight = iframe.contentWindow.document.documentElement.scrollHeight; 
var height = Math.max(bHeight, dHeight); 
iframe.height = height; 
}catch (ex){} 
} 
window.setInterval("reinitIframe1()", 100);   //每隔100毫秒，系统自动执行一次这个reinitIframe()函数
function reinitIframe2(){ 
var iframe = document.getElementById("userpageright"); 
try{ 
var bHeight = iframe.contentWindow.document.body.scrollHeight; 
var dHeight = iframe.contentWindow.document.documentElement.scrollHeight; 
var height = Math.max(bHeight, dHeight); 
iframe.height = height; 
}catch (ex){} 
} 
window.setInterval("reinitIframe2()", 100); 

//-->
</SCRIPT>


